{
  "name": "get-sendy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "keystone postinstall",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract:i18n": "formatjs extract \"{pages,components,sections}/**/*.{js,jsx,ts,tsx}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "@coreui/coreui": "^4.1.5",
    "@coreui/icons": "^2.1.0",
    "@coreui/icons-react": "^2.0.1",
    "@coreui/react": "^4.2.2",
    "@fontsource/plus-jakarta-sans": "^4.5.8",
    "@keystone-6/core": "^2.0.0",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.31.0",
    "next": "12.1.6",
    "nodemailer": "^6.7.5",
    "openai": "^3.0.0",
    "openai-api": "^1.3.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "^6.0.5",
    "sass": "^1.52.1"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.2",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "babel-plugin-formatjs": "^10.3.25",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "typescript": "^4.7.3"
  }
}
